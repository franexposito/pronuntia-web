<% var stylesheet = "login" %>
<% var title = "Iniciar sesión" %>
<% var js = "login" %>
<% include ../header %>

  <nav class="nav">
    <div class="blank">

    </div>
    <div class="titulo-pronuntia">
      <h1>Pronuntia</h1>
    </div>
    <div class="ini">
      <a href="/registro">¿Aún no tienes cuenta? Regístrate</a>
    </div>
  </nav>

  <section class="col-sm-4 col-sm-offset-4 login-cont">
    <div class="row">
      <div class="col-sm-12 formulario">
        <div class="col-sm-12 title">
         <h1>Iniciar sesión</h1>
        </div>
        <form id="login-form" class="col-sm-12">
          <div class="row">
            <div class="col-sm-12 form">
              <div class="form-group">
                <label class="sr-only" for="username_p"></label>
                <input type="text" class="form-control" name="username_p" id="username_p"  placeholder="Usuario">
                <div class="dot"></div>
              </div>
              <div class="form-group">
                <label class="sr-only" for="c1"></label>
                <input type="password" class="form-control" name="c1" id="c1" placeholder="Contraseña">
                <div class="dot"></div>
              </div>
            </div>
            <div class="col-sm-12 bot">
              <button type="submit" class="btn btn-default btn-block btn-sub">Iniciar sesión</button>
            </div>
          </div>
        </form>
      </div>
    </div>
  </section>


<% include ../footer %>
